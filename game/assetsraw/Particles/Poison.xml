<Effect xmlns:meta="Editor">
	<Loop>true</Loop>
	<Emitters>
		<Emitter>
			<Name>Bubbles</Name>
			<IsBlockingEmitter>false</IsBlockingEmitter>
			<Shape>Cone</Shape>
			<Angle>90</Angle>
			<Width>0.2</Width>
			<Height>0.2</Height>
			<SizeMultiplier>
				<Keyframe>0|1</Keyframe>
			</SizeMultiplier>
			<Offset>
				<Keyframe>0|0,0.5</Keyframe>
			</Offset>
			<ParticleSpeed>0.1,0.3</ParticleSpeed>
			<ParticleRotation>0,360</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|0.7</Keyframe>
			</RateKeyframes>
			<Particles>
				<Particle>
					<Lifetime>0.8,1.8</Lifetime>
					<MaintainAspectRatio>true</MaintainAspectRatio>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|0.1|0.2</Keyframe>
						</Stream>
					</SizeKeyframes>
					<BlendMode>Multiplicative</BlendMode>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/glow</Keyframe>
						</Stream>
					</TextureKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|123,5,130,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|0</Keyframe>
							<Keyframe>0.1849831|0.5</Keyframe>
							<Keyframe>0.7628234|0.5</Keyframe>
							<Keyframe>1|0</Keyframe>
						</Stream>
					</AlphaKeyframes>
					<AlphaRefKeyframes>
						<Stream>
							<Keyframe>0|0</Keyframe>
						</Stream>
					</AlphaRefKeyframes>
				</Particle>
			</Particles>
		</Emitter>
	</Emitters>
</Effect>