<Effect xmlns:meta="Editor">
	<Loop>false</Loop>
	<Emitters>
		<Emitter>
			<Name>scratch</Name>
			<Width>1</Width>
			<Height>1</Height>
			<SizeMultiplier>
				<Keyframe>0|1</Keyframe>
			</SizeMultiplier>
			<ParticleSpeed>0,1</ParticleSpeed>
			<ParticleRotation>0,360</ParticleRotation>
			<RateKeyframes>
				<Keyframe>0|1</Keyframe>
			</RateKeyframes>
			<Particles>
				<Particle>
					<Lifetime>1,1</Lifetime>
					<SizeKeyframes>
						<Stream>
							<Keyframe>0|1|1</Keyframe>
						</Stream>
					</SizeKeyframes>
					<BlendMode>Additive</BlendMode>
					<TextureKeyframes>
						<Stream>
							<Keyframe>0|Particle/glow</Keyframe>
						</Stream>
					</TextureKeyframes>
					<ColourKeyframes>
						<Stream>
							<Keyframe>0|255,255,255,255</Keyframe>
						</Stream>
					</ColourKeyframes>
					<AlphaKeyframes>
						<Stream>
							<Keyframe>0|1</Keyframe>
						</Stream>
					</AlphaKeyframes>
					<AlphaRefKeyframes>
						<Stream>
							<Keyframe>0|0</Keyframe>
						</Stream>
					</AlphaRefKeyframes>
				</Particle>
			</Particles>
		</Emitter>
	</Emitters>
</Effect>