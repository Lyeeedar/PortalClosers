<Entity xmlns:meta="Editor">
	<Components>
		<AI>
			<classID>AI</classID>
			<AiPath>AI/player</AiPath>
		</AI>
		<Position>
			<classID>Position</classID>
			<Slot>Entity</Slot>
			<CanSwap>true</CanSwap>
		</Position>
		<Statistics>
			<classID>Statistics</classID>
			<BaseStatistics>
				<Max_hp>100</Max_hp>
				<Power>100</Power>
				<Armour>100</Armour>
			</BaseStatistics>
			<Faction>player</Faction>
			<BloodColour>255,0,0,255</BloodColour>
			<Element>Poison</Element>
		</Statistics>
		<LightComponent>
			<classID>Light</classID>
			<Light meta:RefKey="Light">
				<Colour>255,255,255,255</Colour>
				<Brightness>0.2</Brightness>
			</Light>
		</LightComponent>
		<RenderableComponent>
			<classID>Renderable</classID>
			<Renderable meta:RefKey="Skeleton">
				<Path>Sprites/Skeletons/pc_male/pc_male</Path>
				<AnimGraph>Entities/player_anim</AnimGraph>
				<Colour>255,255,255,255</Colour>
			</Renderable>
		</RenderableComponent>
		<Equipment>
			<classID>Equipment</classID>
			<Weapon>Weapons/BlastAxe</Weapon>
			<ElementalCore>Cores/FireCore</ElementalCore>
			<HealingItem>HealingItems/HealthPotion</HealingItem>
		</Equipment>
	</Components>
</Entity>