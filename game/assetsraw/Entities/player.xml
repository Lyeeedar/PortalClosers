<Entity xmlns:meta="Editor">
	<Components>
		<AI>
			<classID>AI</classID>
			<AiPath>AI/player</AiPath>
		</AI>
		<Position>
			<classID>Position</classID>
			<Slot>Entity</Slot>
			<CanSwap>true</CanSwap>
		</Position>
		<Statistics>
			<classID>Statistics</classID>
			<BaseStatistics>
				<Max_hp>10000</Max_hp>
				<Power>100</Power>
				<Armour>100</Armour>
			</BaseStatistics>
			<Faction>player</Faction>
			<BloodColour>255,0,0,255</BloodColour>
			<Element>Poison</Element>
		</Statistics>
		<LightComponent>
			<classID>Light</classID>
			<Light meta:RefKey="Light">
				<Colour>255,255,255,255</Colour>
				<Brightness>0.2</Brightness>
				<Range>3</Range>
			</Light>
		</LightComponent>
		<RenderableComponent>
			<classID>Renderable</classID>
			<Renderable meta:RefKey="Skeleton">
				<Path>goblin</Path>
				<Scale>0.5</Scale>
			</Renderable>
		</RenderableComponent>
		<Weapon>
			<classID>Weapon</classID>
			<Weapon>Weapons/BlastAxe</Weapon>
		</Weapon>
	</Components>
</Entity>